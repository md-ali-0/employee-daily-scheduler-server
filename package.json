{
  "name": "employee-daily-scheduler-server",
  "version": "1.0.0",
  "description": "A robust Node.js enterprise server boilerplate with Express, TypeScript, MongoDB, Redis, and more.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:dev": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "build:deploy": "pnpm build",
    "test": "jest --passWithNoTests --runInBand",
    "test:watch": "jest --watch --passWithNoTests --runInBand",
    "test:coverage": "jest --coverage --passWithNoTests --runInBand",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,apps,libs,tests}/**/*.ts\"",
    "generate:module": "node scripts/generate-module.js",
    "generate:module:interactive": "node scripts/generate-module.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
